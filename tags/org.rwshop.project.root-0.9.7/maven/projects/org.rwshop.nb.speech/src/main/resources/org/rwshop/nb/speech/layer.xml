        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="SpeechTopComponent.settings" url="SpeechTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
